<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.Accomplist"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="17"/>
    <application android:label="@string/app_name" android:icon="@drawable/ic_launcher">
        <activity android:name="MyActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="MasterScreen"
                  android:label="@string/appTitle">
            <intent-filter>
                <action android:name="com.example.Accomplist.MASTERSCREEN"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name="MainScreen"
                  android:label="@string/appTitle">
            <intent-filter>
                <action android:name="com.example.Accomplist.MAINSCREEN"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name="ViewEvent"
                  android:label="@string/appTitle">
            <intent-filter>
                <action android:name="com.example.Accomplist.VIEWEVENT"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name="UserDetailsActivity"
                  android:label="@string/appTitle">
            <intent-filter>
                <action android:name="com.example.Accomplist.USERDETAILSACTIVITY"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name="SearchActivity"
                  android:label="@string/appTitle">
            <intent-filter>
                <action android:name="com.example.Accomplist.SEARCHACTIVITY"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name="CreditsScreen"
                  android:label="@string/appTitle">
            <intent-filter>
                <action android:name="com.example.Accomplist.CREDITSSCREEN"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
    <activity android:name="AboutScreen"
              android:label="@string/appTitle">
        <intent-filter>
            <action android:name="com.example.Accomplist.ABOUTSCREEN"/>
            <category android:name="android.intent.category.DEFAULT"/>
        </intent-filter>
    </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest>
